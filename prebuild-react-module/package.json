{
    "name"            : "prebuild-react-module",
    "version"         : "0.1.0",
    "private"         : true,
    "dependencies"    : {
        "module-glue-ts"   : "0.0.16",
        "react"            : "^16.6.1",
        "react-dom"        : "^16.6.1",
        "react-scripts-ts" : "3.1.0"
    },
    "scripts"         : {
        "start"         : "cp src/index--default.tsx src/index.tsx && react-scripts-ts start",
        "test"          : "cp src/index--default.tsx src/index.tsx && react-scripts-ts test --env=jsdom",
        "eject"         : "cp src/index--default.tsx src/index.tsx && react-scripts-ts eject",
        "build-default" : "cp src/index--default.tsx src/index.tsx && react-scripts-ts build",
        "build"         : "cp src/index--module.tsx src/index.tsx && react-scripts-ts build"
    },
    "devDependencies" : {
        "@types/jest"      : "^23.3.9",
        "@types/node"      : "^10.12.2",
        "@types/react"     : "^16.4.18",
        "@types/react-dom" : "^16.0.9",
        "typescript"       : "^3.1.6"
    }
}
